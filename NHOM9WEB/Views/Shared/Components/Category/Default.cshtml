@model IList<TbProductCategory>
@using NHOM9WEB.Models;
@using NHOM9WEB.Utilities;

<div class="col-lg-3">
    <div class="left_sidebar_area">
        <aside class="left_widgets p_filter_widgets">
            <div class="l_w_title">
                <h3>Loại sản phẩm</h3>
            </div>
            <div class="widgets_inner">
                <ul class="list">
                    @foreach (var item in Model) {
                        <li>
                            @Html.ActionLink(item.Type, "SanPhamTheoLoai", "Home", new { @Id = item.CategoryProductId }, new { @class = "category-link", data_category_id = item.CategoryProductId })
                        </li>
                    }

                </ul>
            </div>
        </aside>
    </div>
</div>